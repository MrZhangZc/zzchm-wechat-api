{
  port: 4396,
  log4: {
    config: {
      appenders: {
        dateFile: {
          type: "dateFile",
          filename: "logs/default.log",
          pattern: ".yyyy-MM-dd",
          keepFileExt: true,
          compress: true,
        },
        http: {
          type: "dateFile",
          filename: "logs/http.log",
          pattern: ".yyyy-MM-dd",
          keepFileExt: true,
          compress: true,
        },
        info: {
          type: "dateFile",
          filename: "logs/info.log",
          pattern: ".yyyy-MM-dd",
          keepFileExt: true,
          compress: true,
        },
        redis: {
          type: "dateFile",
          filename: "logs/redis.log",
          pattern: ".yyyy-MM-dd",
          keepFileExt: true,
          compress: true,
        },
        mysql: {
          type: "dateFile",
          filename: "logs/mysql.log",
          pattern: ".yyyy-MM-dd",
          keepFileExt: true,
          compress: true,
        },
        task: {
          type: "dateFile",
          filename: "logs/task.log",
          pattern: ".yyyy-MM-dd",
          keepFileExt: true,
          compress: true,
        },
      },
      categories: {
        default: {
          appenders: ["dateFile"],
          level: "info",
        },
        http: {
          appenders: ["http"],
          level: "info",
        },
        info: {
          appenders: ["info"],
          level: "info",
        },
        redis: {
          appenders: ["redis"],
          level: "info",
        },
        mysql: {
          appenders: ["mysql"],
          level: "info",
        },
        task: {
          appenders: ["task"],
          level: "info",
        },
      },
      pm2: true,
    },
    format: ':remote-addr :method :url :status :content-length :response-time ms ":user-agent"',
  },
}
